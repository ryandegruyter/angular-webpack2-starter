{
  "name": "angular-webpack2-starter",
  "version": "1.4.1",
  "description": "Angular and Webpack 2 Starter",
  "author": "Qdouble <qdouble@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qdouble/angular-webpack2-starter.git"
  },
  "scripts": {
    "rimraf": "rimraf",
    "webdev": "webpack-dev-server",
    "webpack": "webpack",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json && npm run rimraf -- output",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/app/**/*.ts\" ",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist .awcache && npm run clean:compile",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "clean:compile": "npm run rimraf -- \"src/compiled\" ",
    "compile": "npm run compile:aot",
    "compile:aot": "npm run clean:compile && npm run ngc && npm run build:aot",
    "compile:dev": "npm run clean:compile && npm run ngc && npm run build:aot:dev",
    "compile:only": "npm run clean:compile && npm run ngc",
    "prodserver": "node prodserver",
    "start": "npm run server:dev",
    "server:dev": "npm run webdev",
    "server:prod": "npm-run-all -p -r watch:prod prodserver",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "build": "npm run build:prod",
    "build:aot": "npm run build:aot:prod",
    "build:aot:prod": "npm run clean:dist && webpack",
    "build:aot:dev": "npm run clean:dist && webpack",
    "build:dev": "npm run clean:dist && webpack",
    "build:prod": "npm run clean:dist && webpack",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch"
  },
  "dependencies": {
    "@angular/common": "2.1.0-rc.0",
    "@angular/compiler": "2.1.0-rc.0",
    "@angular/compiler-cli": "2.1.0-rc.0",
    "@angular/core": "2.1.0-rc.0",
    "@angular/forms": "2.1.0-rc.0",
    "@angular/http": "2.1.0-rc.0",
    "@angular/platform-browser": "2.1.0-rc.0",
    "@angular/platform-browser-dynamic": "2.1.0-rc.0",
    "@angular/router": "3.1.0-rc.0",
    "@angular/tsc-wrapped": "0.3.0",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "~0.9.33",
    "@types/node": "~6.0.45",
    "angular2-router-loader": "~0.3.0",
    "angular2-template-loader": "~0.5.0",
    "awesome-typescript-loader": "~2.2.4",
    "codelyzer": "~1.0.0-beta.0",
    "compression": "1.6.2",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "~0.25.0",
    "es6-promise": "~3.3.0",
    "es6-promise-loader": "~1.0.2",
    "express": "4.14.0",
    "html-webpack-plugin": "~2.22.0",
    "json-loader": "~0.5.4",
    "npm-run-all": "~3.1.0",
    "raw-loader": "~0.5.1",
    "rimraf": "~2.5.4",
    "source-map-loader": "~0.1.5",
    "string-replace-loader": "~1.0.5",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.1",
    "ts-loader": "~0.8.2",
    "ts-node": "~1.3.0",
    "tslint": "~3.15.1",
    "typescript": "~2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "~1.8.1",
    "webpack-dev-server": "2.1.0-beta.4"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/qdouble/angular-webpack2-starter/issues"
  },
  "homepage": "https://github.com/qdouble/angular-webpack2-starter#readme"
}
